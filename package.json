{
  "name": "@becomes/cms-client",
  "version": "2.0.3",
  "description": "Utility for Becomes CMS used for client side implementation.",
  "author": "becomes.co",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build:ts": "tsc -p .",
    "bundle": "node tools.js --bundle",
    "publish": "node tools.js --publish",
    "link": "node tools.js --link",
    "unlink": "node tools.js --unlink",
    "typedoc-generate": "typedoc",
    "test": "mocha -r ts-node/register test/integration/*.test.ts --recursive --exit",
    "test:active": "mocha -r ts-node/register test/integration/*.active.test.ts --recursive --exit",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts --recursive --exit",
    "test:active:unit": "mocha -r ts-node/register test/unit/*.active.test.ts --recursive --exit"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto-js": "^3.1.9-1",
    "socket.io-client": "^2.4.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.14",
    "@types/crypto-js": "^3.1.47",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.19.13",
    "@types/socket.io-client": "^1.4.35",
    "@types/uuid": "^3.4.9",
    "chai": "^4.2.0",
    "env-cmd": "^10.0.1",
    "fs-extra": "^9.0.1",
    "mocha": "^7.2.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.8",
    "typedoc-neo-theme": "^1.0.10",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becomesco/cms-client.git"
  },
  "bugs": {
    "url": "https://github.com/becomesco/cms-client/issues"
  },
  "homepage": "https://github.com/becomesco/cms-client#readme"
}
