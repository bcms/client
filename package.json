{
  "name": "@becomes/cms-client",
  "version": "3.4.1",
  "description": "Make external communication with BCMS easy.",
  "author": "becomes.co",
  "license": "MIT",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "scripts": {
    "build:ts:mjs": "tsc --project tsconfig.json",
    "build:ts:cjs": "tsc --project tsconfig.commonjs.json",
    "build": "npm-tool --build",
    "bundle": "npm-tool --bundle",
    "bundle:local": "npm-tool --local-dev-bundle",
    "link": "npm-tool --link",
    "link:sudo": "npm-tool --link --sudo",
    "unlink": "npm-tool --unlink",
    "unlink:sudo": "npm-tool --unlink --sudo",
    "publish": "npm-tool --publish",
    "pack": "npm-tool --pack",
    "pack:local": "npm-tool --local-dev-pack",
    "create:image": "npm-tool --create-image",
    "typedoc": "typedoc",
    "t": "ts-node -r tsconfig-paths/register src/test.ts",
    "test": "mocha -r ts-node/register test/integration/*.test.ts --recursive --exit",
    "test:active": "mocha -r ts-node/register test/integration/*.active.test.ts --recursive --exit",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts --recursive --exit",
    "test:active:unit": "mocha -r ts-node/register test/unit/*.active.test.ts --recursive --exit"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "socket.io-client": "^4.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@banez/child_process": "^1.0.3",
    "@banez/fs": "^1.0.3",
    "@banez/npm-tool": "^1.1.0",
    "@banez/string-utility": "^1.0.1",
    "@types/chai": "^4.2.18",
    "@types/crypto-js": "^4.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.6.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "chai": "^4.3.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becomesco/cms-client.git"
  },
  "bugs": {
    "url": "https://github.com/becomesco/cms-client/issues"
  },
  "homepage": "https://github.com/becomesco/cms-client#readme"
}
